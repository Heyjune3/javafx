<BorderPane prefHeight="215.0" prefWidth="305.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1"
fx:controller="practice.TableController">
   <bottom>
      <HBox alignment="CENTER" prefHeight="34.0" prefWidth="251.0" BorderPane.alignment="CENTER">
