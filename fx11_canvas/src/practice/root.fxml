         </children>
      </HBox>
   </bottom>
   <center>
      <TableView fx:id="tableView" prefHeight="181.0" prefWidth="346.0" BorderPane.alignment="CENTER">
        <columns>
          <TableColumn fx:id="name" prefWidth="68.0" text="이름" />
          <TableColumn fx:id="ko" prefWidth="58.0" text="국어" />
            <TableColumn fx:id="math" prefWidth="79.0" text="수학" />
            <TableColumn fx:id="eng" prefWidth="71.0" text="영어" />
        </columns>
      </TableView>
   </center>
</BorderPane>
